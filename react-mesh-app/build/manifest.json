{
  "name": "React x Mesh Ext",
  "description": "Testing React x Google Ext",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "React x Mesh Ext",
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo192.png",
      "128": "logo192.png"
    }
  },
  "background": {
    "service_worker": "background.bundle.js",
    "type": "module"
},
  "permissions": [
    "activeTab",
    "http://localhost:3000/api/*",
    "https://*.meshconnect.com/*"
  ]
}
